<Window x:Class="SimpleInventory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleInventory"
        xmlns:viewModels="clr-namespace:SimpleInventory.ViewModels"
        mc:Ignorable="d"
        Title="Simple Inventory" Height="450" Width="800" MinHeight="480" MinWidth="680">
    <Window.DataContext>
        <viewModels:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <ContentPresenter Content="{Binding CurrentViewModel}"/>
    </Grid>
</Window>
